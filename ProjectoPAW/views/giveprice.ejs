<div class="row mt-5">
    <div class="col-md-6 m-auto">
      <div class="card card-body">
        <h1 class="text-center mb-3">Propor preço</h1>
        <% include ./partials/messages %>
        <form action="/leiloes/giveprice" method="POST">
          <div class="form-group">
            <label for="valor">Valor</label>
            <input type="valor" id="valor" name="valor" class="form-control" placeholder="Valor que deseja licitar"
              value="<%= typeof valor != 'undefined' ? valor : '' %>" />
          </div>
          <form action="/leiloes/giveprice" method="POST">

            <div class="form-group">
                <label for="leilao">Leilao a qual está licitar</label>
                <input readonly type="leilao" id="leilao" name="leilao" class="form-control" placeholder="Será associada o preço ao leilao."
                value='<%= id_tlmv %>' value="<%= typeof leilao != 'undefined' ? leilao : '' %>" />
              </div>
            <button type="submit" class="btn btn-primary btn-block">
              Propor preço inicial
            </button>
          </form>
          <p class="lead mt-2"><a href="/dashboardFunc">Voltar</a></p>
      </div>
    </div>
  </div>
  